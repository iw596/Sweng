<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <BorderPane prefHeight="480.0" prefWidth="640.0" BorderPane.alignment="CENTER">
         <top>
            <HBox prefHeight="100.0" prefWidth="640.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Text>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="List Title" textAlignment="CENTER" wrappingWidth="639.13671875">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Text>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="List Items:" underline="true">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Text>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </top>
         <center>
            <ScrollPane fx:id="scroll_pane" prefHeight="138.0" prefWidth="640.0" BorderPane.alignment="CENTER">
               <content>
                  <VBox fx:id="items_pane" alignment="CENTER" prefHeight="130.0" prefWidth="625.0" />
               </content>
            </ScrollPane>
         </center>
         <bottom>
            <BorderPane prefHeight="237.0" prefWidth="640.0" BorderPane.alignment="CENTER">
               <top>
                  <HBox prefHeight="28.0" prefWidth="640.0" BorderPane.alignment="CENTER">
                     <children>
                        <VBox prefHeight="47.0" prefWidth="630.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Most Recent Results:" underline="true">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Text>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </top>
               <center>
                  <ScrollPane prefHeight="108.0" prefWidth="640.0" BorderPane.alignment="CENTER">
                     <content>
                        <VBox prefHeight="120.0" prefWidth="624.0" />
                     </content>
                  </ScrollPane>
               </center>
               <bottom>
                  <HBox alignment="CENTER" prefHeight="65.0" prefWidth="640.0" BorderPane.alignment="CENTER">
                     <children>
                        <JFXButton fx:id="button" onMouseClicked="#button_pressed" text="Complete List" />
                     </children>
                  </HBox>
               </bottom>
            </BorderPane>
         </bottom></BorderPane>
   </center>
</BorderPane>
